{
  "name": "workflow-user-home",
  "version": "0.0.5",
  "description": "User workflow home directory, generated by workflow-cmd",
  "main": "cli.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "node cli.js"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "react": "^16.2.0",
    "workflow-core": "0.0.4",
    "workflow-react": "0.0.5",
    "workflow-transformer-apply-arguments-to-fields": "0.0.1"
  },
  "optionalDependencies": {
    "workflow-wm-i3": "0.0.1",
    "workflow-wm-wmctrl": "0.0.1",
    "workflow-wm-windows": "0.0.1",
    "workflow-wm-osx": "0.0.1"
  }
}
