{
  "name": "workflow-user-home",
  "version": "{{workflow.home.version}}",
  "description": "User workflow home directory, generated by create-workflow-home",
  "main": "cli.js",
  "license": "MIT",
  "keywords": [
    "workflow-wm"
  ],
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "node cli.js"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "react": "^16.2.0",
    "workflow-core": "{{workflow-core}}",
    "workflow-react": "{{workflow-react}}",
    "{{workflow.wm.name}}": "{{workflow.wm.version}}",
    "workflow-layout": "{{workflow-layout}}",
    "workflow-layout-tiled": "{{workflow-layout-tiled}}",
    "workflow-apps-defaults": "{{workflow-apps-defaults}}",
    "workflow-transformer-apply-arguments-to-fields": "{{workflow-transformer-apply-arguments-to-fields}}",
    "workflow-resolver-relative": "{{workflow-resolver-relative}}",
    "workflow-resolver-absolute": "{{workflow-resolver-absolute}}",
    "workflow-loader-babel": "{{workflow-loader-babel}}",
    "workflow-parser-arguments": "{{workflow-parser-arguments}}"
  }
}
