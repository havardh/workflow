{
  "name": "workflow-user-home",
  "version": "{{workflow.home.version}}",
  "description": "User workflow home directory, generated by create-workflow-home",
  "main": "cli.js",
  "license": "MIT",
  "keywords": [
    "workflow-wm"
  ],
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "node cli.js"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "react": "^16.2.0",
    "workflow-core": "1.0.0",
    "workflow-react": "1.0.0",
    "{{workflow.wm.name}}": "{{workflow.wm.version}}",
    "workflow-layout": "1.0.0",
    "workflow-layout-tiled": "1.0.1",
    "workflow-apps-defaults": "0.3.2",
    "workflow-transformer-apply-arguments-to-fields": "1.0.0",
    "workflow-resolver-relative": "1.0.0",
    "workflow-resolver-absolute": "1.0.0",
    "workflow-loader-babel": "1.0.0",
    "workflow-parser-arguments": "1.0.0"
  }
}
