{
  "name": "workflow-app-iterm",
  "version": "1.0.0",
  "description": "iTerm app wrapper for workflow.",
  "main": "index.js",
  "repository": "https://github.com/havardh/workflow",
  "author": "Håvard Wormdal Høiby <havardwhoiby@gmail.com>",
  "keywords": [
    "workflow-wm",
    "iterm",
    "workflow"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "index.js"
  ],
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "modules": false,
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ],
    "env": {
      "test": {
        "presets": [
          "flow",
          "react",
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ],
        "plugins": [
          "transform-object-rest-spread",
          "transform-class-properties"
        ]
      }
    }
  },
  "scripts": {
    "example": "node ../workflow/cli.js --config ../workflow-home-example/config.js Example.js"
  }
}
