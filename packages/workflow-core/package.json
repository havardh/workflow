{
  "name": "workflow-core",
  "version": "0.0.1",
  "description": "Workflow-centric workspace manager",
  "main": "cli.js",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-flow-strip-types",
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "transform-react-jsx"
    ]
  },
  "jest": {
    "testMatch": [
      "**/test/**/*_tests.js"
    ]
  },
  "scripts": {
    "start": "node cli.js",
    "test:all": "jest ",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havardh/workflow.git"
  },
  "keywords": [
    "wm",
    "i3",
    "osx",
    "windows"
  ],
  "author": "Håvard Wormdal Høiby <havardwhoiby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/havardh/workflow/issues"
  },
  "homepage": "https://github.com/havardh/workflow#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "flow-bin": "^0.44.2",
    "jest-junit": "^3.4.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "babel-core": "^6.26.0",
    "babel-plugin-module-rewrite": "^0.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "desktop-screenshot": "^0.1.1",
    "fs-extra": "^3.0.1",
    "i3": "^0.2.1",
    "jest": "^20.0.3",
    "jest-image-snapshot": "^2.2.1",
    "python-shell": "^0.4.0",
    "shelljs": "^0.7.7",
    "tmp-promise": "^1.0.3"
  }
}
