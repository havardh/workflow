{
  "name": "workflow-core",
  "version": "0.0.3",
  "description": "Workflow-centric workspace manager",
  "main": "index.js",
  "files": [
    "dist",
    "cli.js",
    "index.js"
  ],
  "babel": {
    "presets": [
      "flow",
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "jest": {
    "testMatch": [
      "**/test/**/*_tests.js"
    ]
  },
  "scripts": {
    "start": "node cli.js",
    "prepublishOnly": "npm run build",
    "build": "babel src -d dist",
    "test:all": "jest ",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/havardh/workflow.git"
  },
  "keywords": [
    "wm",
    "i3",
    "osx",
    "windows"
  ],
  "author": "Håvard Wormdal Høiby <havardwhoiby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/havardh/workflow/issues"
  },
  "homepage": "https://github.com/havardh/workflow#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "flow-bin": "^0.44.2",
    "jest-junit": "^3.4.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "desktop-screenshot": "^0.1.1",
    "fs-extra": "^3.0.1",
    "i3": "^0.2.1",
    "jest": "^20.0.3",
    "jest-image-snapshot": "^2.2.1",
    "node-osascript": "^2.0.0",
    "osascript": "^1.2.0",
    "python-shell": "^0.4.0",
    "shelljs": "^0.7.7",
    "tmp-promise": "^1.0.3"
  },
  "optionalDependencies": {
    "ffi": "^2.2.0",
    "ref": "^1.3.5",
    "ref-struct": "^1.1.0"
  }
}
